.price-slider.row{:style => "margin-left: 23%;"}
    .col-sm-12
        .row
            .col-sm-3
                %h4{:class => "great"}=label :feedback, var, title

            %div{:class =>"col-sm-4", :style => "padding-left: 25px;"}

                %div{:class =>"row paw-custom-color"}
                    %div{:class => "col-sm-2", :style => "width: 20%"}
                        %i{:class =>"fa fa-paw fa-2x", :id => "#{id}-first"}
                    %div{:class => "col-sm-2", :style => "width: 20%"}
                        %i{:class =>"fa fa-paw fa-2x", :id => "#{id}-second"}
                    %div{:class => "col-sm-2", :style => "width: 20%"}
                        %i{:class => "fa fa-paw fa-2x", :id => "#{id}-third"}
                    %div{:class => "col-sm-2", :style => "width: 20%"}
                        %i{:class =>"fa fa-paw fa-2x", :id => "#{id}-fourth"}
                    %div{:class => "col-sm-2", :style => "width: 20%"}
                        %i{:class => "fa fa-paw fa-2x", :id => "#{id}-fifth"}
                    %div{:class => "row"}
                        %div{:class => "col-sm-2", :style => "float: left; width: 20%; font-size: 12px; margin-left: 10px;"}
                            #{des1}
                        %div{:class => "col-sm-2", :style => "float: right; width: 20%; font-size: 12px; margin-right: 20px;"}
                            #{des5}

            %div{:class=>"col-sm-1", :style => "margin-left: 15px;"}
                = text_field :feedback, var, :class => "read", :id => id, :value => "0", :readonly => true
